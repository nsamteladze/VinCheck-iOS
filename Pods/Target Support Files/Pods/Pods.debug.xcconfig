GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/EDColor" "${PODS_ROOT}/Headers/Public/MBProgressHUD"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/EDColor" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD"
OTHER_LDFLAGS = -ObjC -l"Pods-CocoaLumberjack" -l"Pods-EDColor" -l"Pods-MBProgressHUD" -framework "CoreGraphics" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods